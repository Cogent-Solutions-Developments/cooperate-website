{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "three"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
 "include": [
  "next-env.d.ts",
  "types/**/*.d.ts",
  "**/*.ts",
  "**/*.tsx",
  ".next/types/**/*.ts"
]

,
  "exclude": [
    "node_modules"
  ]
}
